{
  "name": "@umbraco-ui/uui",
  "version": "0.0.10",
  "license": "MIT",
  "description": "The full Umbraco UI Library. The package includes and registers all UUI Web Components.",
  "keywords": [
    "Umbraco",
    "Custom elements",
    "Web components",
    "UI",
    "Lit",
    "Components library"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/umbraco/Umbraco.UI.git",
    "directory": "packages/uui"
  },
  "bugs": {
    "url": "https://github.com/umbraco/Umbraco.UI/issues"
  },
  "homepage": "https://uui.umbraco.com/",
  "main": "./dist/uui.min.js",
  "module": "./lib/index.js",
  "customElements": "custom-elements.json",
  "files": [
    "dist",
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "custom-elements.json"
  ],
  "dependencies": {
    "@umbraco-ui/uui-action-bar": "0.0.2",
    "@umbraco-ui/uui-avatar": "0.0.16",
    "@umbraco-ui/uui-avatar-group": "0.0.16",
    "@umbraco-ui/uui-badge": "0.0.9",
    "@umbraco-ui/uui-box": "0.0.11",
    "@umbraco-ui/uui-breadcrumbs": "0.0.16",
    "@umbraco-ui/uui-button": "0.1.7",
    "@umbraco-ui/uui-button-group": "0.0.9",
    "@umbraco-ui/uui-caret": "0.0.6",
    "@umbraco-ui/uui-checkbox": "0.0.16",
    "@umbraco-ui/uui-icon": "0.0.2",
    "@umbraco-ui/uui-input": "0.0.16",
    "@umbraco-ui/uui-keyboard-shortcut": "0.0.10",
    "@umbraco-ui/uui-loader": "0.0.16",
    "@umbraco-ui/uui-loader-bar": "0.0.17",
    "@umbraco-ui/uui-loader-circle": "0.0.17",
    "@umbraco-ui/uui-menu-item": "0.0.4",
    "@umbraco-ui/uui-radio": "0.0.17",
    "@umbraco-ui/uui-slider": "0.0.16",
    "@umbraco-ui/uui-symbol-expand": "0.0.2",
    "@umbraco-ui/uui-symbol-file": "0.0.1",
    "@umbraco-ui/uui-symbol-folder": "0.0.1",
    "@umbraco-ui/uui-symbol-sort": "0.0.2",
    "@umbraco-ui/uui-table": "0.0.16",
    "@umbraco-ui/uui-tabs": "0.0.16",
    "@umbraco-ui/uui-tag": "0.0.16",
    "@umbraco-ui/uui-text-area": "0.0.1",
    "@umbraco-ui/uui-toggle": "0.0.16"
  },
  "scripts": {
    "build": "npm run analyze && tsc --build --force && rollup -c rollup.config.js",
    "clean": "tsc --build --clean && rimraf dist lib/*.js lib/**/*.js custom-elements.json",
    "analyze": "web-component-analyzer **/*.element.ts --outFile custom-elements.json"
  },
  "publishConfig": {
    "access": "public"
  }
}
