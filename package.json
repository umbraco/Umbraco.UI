{
  "name": "@umbraco-ui/uui",
  "version": "1.17.0",
  "description": "The Umbraco UI Library â€” web components for the Umbraco backoffice.",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./components/*": "./dist/components/*",
    "./internal/*": "./dist/internal/*",
    "./themes/*": "./dist/themes/*",
    "./styles/*": "./dist/styles/*"
  },
  "customElements": "custom-elements.json",
  "files": [
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.d.ts",
    "dist/**/*.css",
    "dist/**/*.woff2",
    "custom-elements.json",
    "vscode.html-custom-data.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/umbraco/Umbraco.UI.git"
  },
  "bugs": {
    "url": "https://github.com/umbraco/Umbraco.UI/issues"
  },
  "homepage": "https://github.com/umbraco/Umbraco.UI/#readme",
  "keywords": [
    "umbraco",
    "backoffice",
    "web-components",
    "shadow-dom",
    "custom-elements"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint:eslint": "eslint --cache",
    "format:eslint": "eslint --fix",
    "lint:prettier": "prettier \"**/*.js\" \"**/*.ts\" \"**/*.mjs\" --check --ignore-path .gitignore",
    "format:prettier": "prettier \"**/*.js\" \"**/*.ts\" \"**/*.mjs\"  --write --ignore-path .gitignore",
    "lint": "npm run lint:eslint",
    "format": "npm run format:eslint",
    "test": "web-test-runner --coverage",
    "test:watch": "web-test-runner --watch",
    "test:coverage-for": "node scripts/test-coverage-package.js",
    "dev": "npm run clean && npm run storybook",
    "build": "vite build && tsc -p tsconfig.build.json && npm run analyze",
    "build:watch": "vite build --watch",
    "clean": "rimraf dist custom-elements.json vscode.html-custom-data.json",
    "analyze": "web-component-analyzer src/**/*.element.ts --outFile custom-elements.json && web-component-analyzer src/**/*.element.ts --format vscode --outFile vscode.html-custom-data.json",
    "prepare": "husky",
    "chromatic": "npx chromatic --build-script-name storybook:build",
    "storybook": "npm run analyze && storybook dev -p 6006",
    "storybook:build": "npm run analyze && storybook build",
    "new-component": "plop"
  },
  "engines": {
    "node": ">=24.13",
    "npm": ">=11"
  },
  "dependencies": {
    "lit": "^3.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "5.0.1",
    "colord": "^2.9.3",
    "@eslint/compat": "2.0.2",
    "@eslint/js": "10.0.1",
    "@lerna-lite/cli": "4.11.3",
    "@lerna-lite/publish": "4.11.3",
    "@lerna-lite/version": "4.11.3",
    "@open-wc/testing": "4.0.0",
    "@storybook/addon-a11y": "^10.2.12",
    "@storybook/addon-docs": "^10.2.12",
    "@storybook/addon-links": "^10.2.12",
    "@storybook/web-components-vite": "^10.2.12",
    "@testing-library/dom": "10.4.1",
    "@types/chai": "5.2.3",
    "@types/mocha": "10.0.10",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.1",
    "@web/dev-server-esbuild": "1.0.5",
    "@web/test-runner": "0.20.2",
    "@web/test-runner-commands": "0.9.0",
    "@web/test-runner-playwright": "0.11.1",
    "chromatic": "^15.2.0",
    "eslint": "10.0.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-lit": "2.2.1",
    "eslint-plugin-local-rules": "3.0.2",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-storybook": "10.2.12",
    "eslint-plugin-wc": "3.1.0",
    "github-markdown-css": "5.8.1",
    "globals": "17.3.0",
    "husky": "9.1.7",
    "lint-staged": "16.1.2",
    "plop": "4.0.5",
    "prettier": "3.8.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-markdown": "10.1.0",
    "react-syntax-highlighter": "16.1.0",
    "remark-gfm": "4.0.1",
    "rimraf": "6.1.3",
    "storybook": "^10.2.12",
    "tsc-files": "1.1.4",
    "typescript": "5.9.3",
    "typescript-eslint": "8.55.0",
    "vite": "7.3.1",
    "web-component-analyzer": "2.0.0"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.element.ts": "tsc-files --project tsconfig.lint-staged.json",
    "*": "prettier --ignore-unknown --write"
  },
  "storybook": {
    "url": "https://uui.umbraco.com"
  },
  "packageManager": "npm@11.1.0"
}
