{
  "name": "uui-monorepo",
  "version": "1.0.0",
  "description": "A library of UI-components, build with love from Umbraco",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/umbraco/Umbraco.UI.git"
  },
  "bugs": {
    "url": "https://github.com/umbraco/Umbraco.UI/issues"
  },
  "homepage": "https://github.com/umbraco/Umbraco.UI/#readme",
  "keywords": [
    "umbraco",
    "backoffice",
    "web-components",
    "shadow-dom",
    "custom-elements"
  ],
  "scripts": {
    "lint:eslint": "eslint --cache",
    "format:eslint": "eslint --fix",
    "lint:prettier": "prettier \"**/*.js\" \"**/*.ts\" \"**/*.mjs\" --check --ignore-path .gitignore",
    "format:prettier": "prettier \"**/*.js\" \"**/*.ts\" \"**/*.mjs\"  --write --ignore-path .gitignore",
    "lint": "npm run lint:eslint",
    "format": "npm run format:eslint",
    "test": "turbo run build --filter=@umbraco-ui/uui-css && web-test-runner --coverage",
    "test:watch": "web-test-runner --watch",
    "test:coverage": "web-test-runner --node-resolve --coverage",
    "test:coverage-for": "node ./scripts/test-coverage-package.js",
    "dev": "npm run clean && npm run storybook",
    "build": "turbo run build",
    "build:prod": "npm run clean && npm run build",
    "clean": "turbo run clean --parallel",
    "lerna:publish": "npm run build:prod && npm whoami && lerna publish from-package --yes --pre-dist-tag prerelease",
    "lerna:version": "lerna version --sync-workspace-lock",
    "lerna:version:rc": "npm run lerna:version -- --conventional-prerelease",
    "lerna:changelog": "npx lerna-changelog",
    "lerna:modify-package": "lerna exec -- node ../../scripts/modify-pkgjson.mjs",
    "test:scripts": "node --test scripts/**/*.test.mjs",
    "check-updates": "npx ncu && npx lerna exec --no-bail -- npx ncu",
    "prepare": "husky",
    "new-package": "plop && npm i",
    "pack-all": "npm run build:prod && node ./scripts/pack-packages.js",
    "pack-all-no-build": "node ./scripts/pack-packages.js",
    "pack": "node ./scripts/pack-package.js",
    "chromatic": "npx chromatic --build-script-name storybook:build",
    "storybook": "npm run storybook:analyze && storybook dev -p 6006",
    "storybook:build": "npm run storybook:analyze && storybook build",
    "storybook:analyze": "web-component-analyzer **/*.element.ts --outFile custom-elements.json",
    "postinstall": "node ./scripts/generate-ts-config.js"
  },
  "engines": {
    "node": ">=24.13",
    "npm": ">=11"
  },
  "dependencies": {
    "lit": "^2.8.0"
  },
  "devDependencies": {
    "@babel/core": "7.28.5",
    "@chromatic-com/storybook": "5.0.1",
    "@eslint/compat": "2.0.2",
    "@eslint/js": "10.0.1",
    "@lerna-lite/cli": "4.11.0",
    "@lerna-lite/exec": "4.11.1",
    "@lerna-lite/publish": "4.11.1",
    "@lerna-lite/run": "4.11.1",
    "@lerna-lite/version": "4.11.1",
    "@open-wc/testing": "4.0.0",
    "@rollup/plugin-node-resolve": "16.0.3",
    "@storybook/addon-a11y": "^10.2.8",
    "@storybook/addon-docs": "^10.2.8",
    "@storybook/addon-links": "^10.2.8",
    "@storybook/web-components-vite": "^10.2.8",
    "@testing-library/dom": "10.4.1",
    "@types/chai": "5.2.3",
    "@types/mocha": "10.0.10",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.1",
    "@web/dev-server-esbuild": "0.4.3",
    "@web/test-runner": "0.20.2",
    "@web/test-runner-commands": "0.9.0",
    "@web/test-runner-playwright": "0.11.1",
    "autoprefixer": "10.4.21",
    "chromatic": "15.1.0",
    "element-internals-polyfill": "3.0.2",
    "esbuild": "0.27.2",
    "eslint": "10.0.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-lit": "2.2.1",
    "eslint-plugin-local-rules": "3.0.2",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-storybook": "10.2.8",
    "eslint-plugin-wc": "3.1.0",
    "github-markdown-css": "5.8.1",
    "glob": "11.1.0",
    "globals": "17.3.0",
    "husky": "9.1.7",
    "lint-staged": "16.1.2",
    "plop": "3.1.2",
    "postcss": "8.5.6",
    "postcss-advanced-variables": "5.0.0",
    "postcss-cli": "11.0.1",
    "postcss-custom-properties": "14.0.6",
    "postcss-import": "16.1.1",
    "postcss-jsx": "0.36.4",
    "postcss-load-config": "6.0.1",
    "postcss-syntax": "0.36.2",
    "postcss-url": "10.1.3",
    "prettier": "3.8.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-markdown": "10.1.0",
    "react-syntax-highlighter": "16.1.0",
    "remark-gfm": "4.0.1",
    "rimraf": "5.0.5",
    "rollup": "4.53.3",
    "rollup-plugin-esbuild": "6.2.1",
    "rollup-plugin-import-css": "3.5.5",
    "rollup-plugin-postcss": "4.0.2",
    "storybook": "^10.2.8",
    "tsc-files": "1.1.4",
    "turbo": "2.7.6",
    "typescript": "5.9.3",
    "typescript-eslint": "8.55.0",
    "vite": "7.3.1",
    "vite-plugin-turbosnap": "1.0.3",
    "web-component-analyzer": "2.0.0"
  },
  "overrides": {
    "lit": "^2.8.0",
    "playwright": "^1.45.3"
  },
  "workspaces": [
    "./packages/*"
  ],
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.element.ts": "tsc-files --project tsconfig.lint-staged.json",
    "*": "prettier --ignore-unknown --write"
  },
  "storybook": {
    "url": "https://uui.umbraco.com"
  },
  "packageManager": "npm@11.1.0"
}
